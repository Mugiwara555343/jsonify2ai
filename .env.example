# Copy to .env on first run (scripts/ensure_tokens.* will auto-generate secrets).

# API <-> Web
API_AUTH_TOKEN=
WORKER_AUTH_TOKEN=

# Qdrant
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION=jsonify2ai_chunks_768
QDRANT_COLLECTION_IMAGES=jsonify2ai_images_768

# Embedder
EMBEDDINGS_MODEL=nomic-embed-text
EMBED_DEV_MODE=0

# Ollama (choose one)
# If Ollama on host:
# OLLAMA_HOST=http://host.docker.internal:11434
# If Ollama as docker service:
# OLLAMA_HOST=http://ollama:11434
LLM_PROVIDER=ollama
OLLAMA_MODEL=qwen2.5:3b-instruct-q4_K_M

# CORS (dev defaults; harden in prod)
API_CORS_ORIGINS=http://localhost:5173
WORKER_CORS_ORIGINS=http://localhost:5173
